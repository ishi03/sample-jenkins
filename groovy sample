pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('Build') {
            steps {
                echo 'This is Build Stage'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'This is Deploy Stage'
            }
        }
        stage('Test') {
            steps {
                echo 'This is in Testing Stage'
            }
        }
        
        stage('Release') {
            steps {
                echo 'This is in Release Stage'
            }
        }
    }
}
